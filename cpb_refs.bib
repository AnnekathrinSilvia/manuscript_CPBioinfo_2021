@incollection{Calura2021,
abstract = {The main purpose of pathway or gene set analysis methods is to provide mechanistic insight into the large amount of data produced in high-throughput studies. These tools were developed for gene expression analyses, but they have been rapidly adopted by other high-throughput techniques, becoming one of the foremost tools of omics research. Currently, according to different biological questions and data, we can choose among a vast plethora of methods and databases. Here we use two published examples of RNAseq datasets to approach multiple analyses of gene sets, networks and pathways using freely available and frequently updated software. Finally, we conclude this chapter by presenting a survival pathway analysis of a multiomics dataset. During this overview of different methods, we focus on visualization, which is a fundamental but challenging step in this computational field.},
address = {New York, NY},
author = {Calura, Enrica and Martini, Paolo},
booktitle = {RNA Bioinformatics},
chapter = {9},
doi = {10.1007/978-1-0716-1307-8},
editor = {Picardi, Ernesto},
file = {:Users/fede/Library/Application Support/Mendeley Desktop/Downloaded/Calura, Martini - 2021 - Summarizing RNA-Seq Data or Differentially Expressed Genes Using Gene Set, Network, or Pathway Analysis.pdf:pdf},
isbn = {9784431538592},
keywords = {Gene set analysis,Multiomics data integration,Networks,Pathway,Topological pathway analysis},
mendeley-groups = {GeneTonic{\_}manuscript,cpbioinfo{\_}manuscript},
pages = {147--179},
publisher = {Humana},
title = {{Summarizing RNA-Seq Data or Differentially Expressed Genes Using Gene Set, Network, or Pathway Analysis}},
url = {https://link.springer.com/10.1007/978-1-0716-1307-8},
volume = {2284},
year = {2021}
}
@article{Triggle2021,
abstract = {Journal impact factors, publication charges and assessment of quality and accuracy of scientific research are critical for researchers, managers, funders, policy makers, and society. Editors and publishers compete for impact factor rankings, to demonstrate how important their journals are, and researchers strive to publish in perceived top journals, despite high publication and access charges. This raises questions of how top journals are identified, whether assessments of impacts are accurate and whether high publication charges borne by the research community are justified, bearing in mind that they also collectively provide free peer-review to the publishers. Although traditional journals accelerated peer review and publication during the COVID-19 pandemic, preprint servers made a greater impact with over 30,000 open access articles becoming available and accelerating a trend already seen in other fields of research. We review and comment on the advantages and disadvantages of a range of assessment methods and the way in which they are used by researchers, managers, employers and publishers. We argue that new approaches to assessment are required to provide a realistic and comprehensive measure of the value of research and journals and we support open access publishing at a modest, affordable price to benefit research producers and consumers.},
author = {Triggle, Chris R and MacDonald, Ross and Triggle, David J and Grierson, Donald},
doi = {10.1080/08989621.2021.1909481},
file = {:Users/fede/Downloads/Requiem for impact factors and high publication charges.pdf:pdf},
issn = {1545-5815},
journal = {Accountability in research},
keywords = {Metrics,for,journal impact factor,journal impact factors,of,open access,publication charges and assessment,publication costs,quality and accuracy of,scientific research are critical},
mendeley-groups = {cpbioinfo{\_}manuscript},
number = {00},
pages = {1--32},
pmid = {33787413},
publisher = {Taylor {\&} Francis},
title = {{Requiem for impact factors and high publication charges.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/33787413},
volume = {00},
year = {2021}
}
