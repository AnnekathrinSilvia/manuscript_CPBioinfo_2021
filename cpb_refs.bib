issn = {14712105},
journal = {BMC Bioinformatics},
keywords = {Analysis,Gene fusion,Immunological infiltrate,Pipeline,RNA-seq,Snakemake},
mendeley-groups = {pcaExplorer{\_}manuscript,ideal{\_}manuscript,cpbioinfo{\_}manuscript},
