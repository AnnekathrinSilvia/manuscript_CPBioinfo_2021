@article{McNutt2014,
abstract = {A common set of Principles and Guidelines in Reporting Preclinical Research (see go.nature.com/ezjl1p) recommend that journals include in their information for authors their policies for statistical analysis and how they review the statistical accuracy of work under consideration. The guidelines encourage using a checklist to ensure reporting of important experimental parameters, such as standards used, number and type of replicates, statistics, method of randomization, whether experiments were blinded, how the sample size was determined and what criteria were used to include or exclude any data. The guidelines suggests that journals establish best practices for dealing with image-based data (for example, screening for manipulation, storing full-resolution archival versions) and for describing experiments in full. An example for animal experiments is to report the source, species, strain, sex, age, husbandry and inbred and strain characteristics for transgenic animals. For cell lines, one might report the source, authentication and mycoplasma contamination status. The existence of these guidelines does not obviate the need for replication or independent verification of research results, but should make it easier to perform such replication.},
author = {McNutt, M.},
doi = {10.1126/science.aaa1724},
