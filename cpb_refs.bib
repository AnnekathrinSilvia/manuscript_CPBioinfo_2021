journal = {bioRxiv},
mendeley-groups = {GeneTonic{\_}manuscript,cpbioinfo{\_}manuscript},
pages = {661728},
title = {{A Single Cell Transcriptomic Atlas Characterizes Aging Tissues in the Mouse}},
