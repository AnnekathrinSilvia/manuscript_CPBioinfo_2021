issn = {1467-5463},
journal = {Briefings in Bioinformatics},
keywords = {bioconductor package,bioinformatics tools,differential gene expression analysis,differentially expressed genes,interpretation,r,visualization and},
mendeley-groups = {ideal{\_}manuscript,GeneTonic{\_}manuscript,cpbioinfo{\_}manuscript},
month = {aug},
